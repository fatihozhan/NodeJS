<div class="card mb-3">
    <div class="row">
      <div class="col-md-3">
        <img
          src="/static/images/<%= blog_img %>"
          alt=""
          class="img-fluid"
        />
      </div>
      <div class="col-md-9">
        <div class="card-body">
          <div class="cart-title">
            <h5><%= blog_title %></h5>
            <p class="card-text"><%= blog_desc %></p>
          </div>
        </div>
      </div>
    </div>
  </div>